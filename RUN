    public static final String ANSI_RESET = "\u001B[0m", ANSI_RED = "\u001B[31m", ANSI_Black = "\u001B[30m", ANSI_Purple = "\u001B[35m", ANSI_Green = "\u001B[32m";

    //method to get user input 
    public static int readInt(String prompt, int userChoices){
        int input;

        do { 
            System.out.print(prompt);
            try { 
                input = Integer.parseInt(sc.next());
                    if (input < 1 || input > userChoices) {
                        System.out.println("Enter a value between 1 and " + userChoices);
                    }
            } catch (Exception e){
                input = -1;
                System.out.println("Enter an integer.");
            }
		}
			while (input < 1 || input > userChoices);
			return input;
	}
 
         
    //method to simulate clearing out the console
    public static void clearConsole(){
        for(int i = 0; i < 100; i++){
            System.out.println("");
        }
        }
    
    //method to print a separator with length n
    public static void printSeperator(int n){
        for (int i = 0; i < n; i++){
            System.out.print(ANSI_RED + "~" +ANSI_RESET);
        }
    }
    //method to print a heading
    public static void printHeading(String title){
        printSeperator(30);
		System.out.println();
        System.out.println(title);
        printSeperator(30);
    }
    //method to stop the game until user enters something
    public static void enterToContinue(){
        System.out.println("\nPress Enter to continue.");
        try
        {
            System.in.read();
            sc.nextLine();
        }  
        catch(Exception e)
        {}           
    }
    
