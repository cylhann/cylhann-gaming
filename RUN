        public static void logs(){
            try{
                FileWriter out = new FileWriter("./rpgProject/log.txt", true);
                if(win == true){
                    out.write(GameLogic.name + " lived to tell the tale.\n");
                }
                    else if(win == false){
                        if(hanako == true){
                        out.write(GameLogic.name + " was killed by Hanako.\n");
                        }
                            else if(dobbi == true){
                                out.write(GameLogic.name + " was killed by Dobbi.\n");
                            }
                                else if(cclown == true){
                                    out.write(GameLogic.name + " was killed by PenniWise.\n");
                                }
                                    else if(deadbat == true){
                                        out.write(GameLogic.name + " was killed by the Darkness.\n");
                                    }
                                        else if(ccorny){
                                            out.write(GameLogic.name + " was killed for being Corny.\n");
                                        }
                                            else if(debil == true){
                                                out.write(GameLogic.name + " was killed by the Devil.\n");
                                            }
                                                else if(jigg == true){
                                                    out.write(GameLogic.name + " was killed by Jiggsaw.\n");
                                                }
                        }
                out.close();
            }
            
            catch (Exception e){
                System.err.println("An error occured while writing the file to: " + e.getMessage());
            } 
        }
        public static void readLogs(){
            final File file = new File( "./rpgProject/log.txt" );

            try ( final Scanner scanner = new Scanner( file ); ) {
            while ( scanner.hasNextLine() ) {
                String line = scanner.nextLine();
                System.out.println( line );
            }
            } catch ( FileNotFoundException e ) {
            e.printStackTrace();
        }
        }

