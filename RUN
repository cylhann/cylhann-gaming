        System.out.println(ANSI_RED + GameLogic.name + ANSI_RESET + ": I need to get out of here...");
        System.out.println("You muttered.");
        GameLogic.enterToContinue();
        GameLogic.clearConsole();
        System.out.println("You went to the door and checked outside. There are no lights except the farthest area which is the comfort room.");
        do{
            scene = false;
        System.out.println("(1) Check for anything useful inside the room before leaving.");
        System.out.println("(2) Leave the classroom.");
        input = readInt("->", 2);
        if(input == 1){
            GameLogic.clearConsole();
            System.out.println("You inspected every nook and cranny. You found nothing of any use.");
            System.out.println("As you looked on the seat where you woke up, you found something");
            System.out.println("You found a " + ANSI_RED + "knife." + ANSI_RESET);
            knife = true;
            scene = true;
            GameLogic.enterToContinue();
            GameLogic.clearConsole();
        }
            else if(input == 2){
                scene = true;
                GameLogic.clearConsole();
            }
        } while(!scene);
    }
    public static void printThirdAct(){
        battery = battery - 2;
        System.out.println("You exited the classroom. The hallway is very dark and only your phone's flashlight is your source of light.\nThe silence is not just deafening but also eerie.");
        System.out.println("Everything that's happening is giving you goosebumps.");
        do{
            scene = false;
        System.out.println("(1) Head towards the stairs and go downstairs.");
        System.out.println("(2) Head towards the comfort room where the light is on.");
        input = readInt("->", 2);
        if(input == 1){
            scene = true;
            GameLogic.clearConsole(); 
        }
            else if(input == 2){
                GameLogic.clearConsole();
                System.out.println("You walked towards the comfort room. There's this feeling you can't scratch.");
                System.out.println("As you walked past the entrance. You see that the men's comfort room is closed and door is shut tight.");
                System.out.println("Moving your gaze on your on your right, you saw the women's comfort room's door and light both open.");
                scene = true;
                GameLogic.clearConsole();
                System.out.println("You entered the comfort room. You can hear a female voice humming.");
                do{
                    scene = false;  
                    System.out.println("(1) Head back and just go to the stairs.");
                    System.out.println("(2) Check the cubicles one by one.");
                    input = readInt("->", 2);
                    if(input == 1){
                        GameLogic.clearConsole();
                        scene = true;
                    }
                        else if(input == 2){
                            GameLogic.clearConsole();
                            System.out.println("You first went to the nearest and first cubicle you saw.");
